# Install and load the gtrendsR package
install.packages("gtrendsR")
library(gtrendsR)
# Define the search term and values for time and product
text <- "data science"   # Search term
values <- list(
time = "2020-01-01 2023-01-01",   # Time range
product = "web"                   # Google product (web, news, images, froogle, youtube)
)
# Fetch Google Trends data
result <- gtrends(text, geo = "US", time = values$time, gprop = values$product)
